<h2>Employee List</h2>
<table id="myTable">
    <thead>
        <tr>
            <th>EmployeeId</th>
            <th>Name</th>
            <th>Age</th>
            <th>Position</th>
        </tr>
    </thead>
</table>

@section scripts {
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.2.1.min.js"></script>
    <script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
    <script>
        $(document).ready(function () {

            $('#myTable').DataTable({
                processing: true,
                "ajax": {
                    "type": "GET",
                    "url": "/DatatableInAnotherWay/GetList",
                    "dataSrc": function (msg) {
                        console.log(msg);
                        return msg;
                    }
                },
                "columns": [
                    { "data": "employeeId" },
                    { "data": "name" },
                    { "data": "age" },
                    { "data": "position" },

                ]
            });

        });
    </script>
}

